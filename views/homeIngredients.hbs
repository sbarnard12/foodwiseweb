<div class="body-container">
    <div class="body-content-container">
        <h4>Update Home Ingredients:</h4>
        <p><b>NOTE: </b>Please enter the ingredients separated by commas. Do not enter quantities. Entering new ingredients will overwrite previous entries</p>
        <form id="homeIngredientsForm">
            <div class="row form-group">
                <div class="col-md-8">
                    <input type="text" name="homeIng" class="form-control" placeholder="Ex: carrots, onions, bread" id="homeIng">
                </div>
                <input type="submit" class="btn btn-default" id="submit_button">
            </div>
        </form>

        <p>Previously entered Ingredients: </p><div id="prevIngList"></div>
    </div>
</div>
<script src="/javascripts/homeIngredientsScript.js"></script>

